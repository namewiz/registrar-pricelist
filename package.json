{
  "name": "registrar-pricelist",
  "version": "0.1.1",
  "description": "Generates structured JSON price lists for TLDs from different registrars like Namecheap and OpenProvider",
  "type": "module",
  "scripts": {
    "cli": "node src/cli/index.js",
    "test": "node --test",
    "prebuild": "mkdir -p dist",
    "build": "npx -y esbuild src/browser/index.js --bundle --format=esm --platform=browser --target=es2020 --sourcemap --outfile=dist/registrar-pricelist.js",
    "postbuild": "mkdir -p docs && cp dist/registrar-pricelist.js docs/registrar-pricelist.js"
  },
  "bin": {
    "registrar-pricelist": "./src/cli/index.js"
  },
  "exports": {
    ".": "./src/index.js",
    "./cli": "./src/cli/index.js",
    "./generators": "./src/generators/index.js",
    "./data/namecheap.json": "./data/namecheap-prices.json",
    "./data/nira.json": "./data/nira-prices.json",
    "./data/openprovider.json": "./data/openprovider-prices.json"
  },
  "homepage": "https://github.com/namewiz/registrar-pricelist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namewiz/registrar-pricelist.git"
  },
  "bugs": {
    "url": "https://github.com/namewiz/registrar-pricelist/issues"
  },
  "author": "Justice Ogbonna <dev@justiceo.com>",
  "keywords": [
    "domain-price",
    "tld-price",
    "registrar-prices",
    "namecheap-price"
  ],
  "license": "MIT",
  "engines": {
    "node": "^22.12.0",
    "npm": "^10.9.0"
  },
  "dependencies": {
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5"
  }
}
